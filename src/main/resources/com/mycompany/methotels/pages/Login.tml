<html t:type="layout" title="MetHotels com.example"
      xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd"
      xmlns:p="tapestry:parameter">

<div class="row">
    <div class="span4 offset3">
        <t:form t:id="form" object="userLogin" exclude="userId, rola, ime, prezime" class="form-signin" submitlabel="Login">
            <h2 class="form-signin-heading">Please sign in</h2>
            <p:sifra>
                <t:label for="sifra"/>
                <t:passwordfield t:id="sifra" value="userLogin.sifra"/>
            </p:sifra>
        </t:form>
    </div>
</div>

</html>
